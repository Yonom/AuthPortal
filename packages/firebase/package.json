{
  "name": "@authportal/firebase",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup"
  },
  "peerDependencies": {
    "@firebase/auth": "^1"
  },
  "devDependencies": {
    "@authportal/core": "*",
    "tsup": "^7.2.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "sourcemap": true,
    "noExternal": [
      "@authportal/core"
    ]
  },
  "files": [
    "dist"
  ]
}
